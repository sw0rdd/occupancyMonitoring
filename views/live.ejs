<div class="live-data">
    <h1>LIVE</h1>
    <% if(response) { %>
        <div>
            <p id="entryCount">Total Count: <%= response.entry %></p>
            <p id="groundFloorCount">Ground Floor: <%= response.groundFloor %></p>
            <p id="firstFloorCount">First Floor: <%= response.firstFloor %></p>
            <p id="secondFloorCount">Second Floor: <%= response.secondFloor %></p>
            <p id="timeStamp">Timestamp: <%= response.timestamp %></p>
        </div>
    <% } else { %>
        <p class="no-data">No data available</p>
    <% } %>
</div>